{
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/JS/"
	},
	"plugins": [],
    "recurseDepth": 10,
    "source": {
		"include": ["./site_assets/js/modules.js", "./site_assets/js/modules/product_search.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}