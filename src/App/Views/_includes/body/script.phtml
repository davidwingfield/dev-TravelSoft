										_modal_product_arrive_to_station_id.value = ""
									}
									if (_modal_product_arrive_to_station) {
										_modal_product_arrive_to_station.value = ""
									}
								}
								
							}
						})
					}
				})
			}
		}
		
		const fetchByName = function (dataToSend, callback) {
			let url = "/api/v1.0/stations/validate"
			
			if (dataToSend) {
				try {
