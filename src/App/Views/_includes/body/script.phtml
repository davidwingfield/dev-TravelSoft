				 *
				 * @type {{name}}
				 */
				let dataToSend = {
					name: name,
				}
				
				fetchByName(dataToSend, function (data) {
					let station = null
					
					if (data) {
						station = data
						if (data[0]) {
							station = data[0]
						}
