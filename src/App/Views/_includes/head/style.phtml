	.picker-content {
		position:           absolute;
		display:            inline-flex;
		line-height:        2rem;
		top:                1rem;
		font-size:          1.15rem;
		-webkit-transition: all .2s ease-in-out;
		transition:         all .2s ease-in-out;
		font-weight:        400;
		text-transform:     initial;
	}
	.btn-product-search-reset-picker-icon {
		cursor:         pointer;
		line-height:    1rem;
		height:         1rem;
		min-width:      24px;
		pointer-events: none;
		width:          2rem;
		font-size:      1rem;
		
	}
