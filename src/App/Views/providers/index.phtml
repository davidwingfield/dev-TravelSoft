		const handleCompanyError = function (msg, title, level) {
			console.log("Company.handleCompanyError(msg)", msg)
			// ----
			
			if (!title) {
				title = "Company"
			}
			
			if (!level) {
				level = "error"
			}
			
			toastr[level](`${msg}`, title)
		}
		
		const fetchCompanyByName = function (dataToSend, callback) {
			console.log("Provider.fetchCompanyByName(dataToSend, callback)", dataToSend, callback)
			// ----
			
			let url = "/api/v1.0/companies/validate"
			
			if (dataToSend) {
				try {
					sendGetRequest(url, dataToSend, function (data, status, xhr) {
						if (data) {
							return callback(data)
						} else {
							return handleCompanyError("Oops: 1", "Company", "error")
						}
					})
				} catch (e) {
					return handleCompanyError("Error Validating Company", "Company", "error")
				}
			} else {
				return handleCompanyError("Error Loading Company- Missing Data", "Company", "error")
			}
		}
		
		const companyExists = function (companyName) {
			console.log("Provider.companyExists(companyName)", companyName)
			// ----
			
			if (companyName) {
				fetchCompanyByName({ name: companyName }, function (data) {
					if (data) {
						let company = (data[0]) ? data[0] : data
						console.log("|__ |__ company", company)
					}
				})
			}
		}
		
		const initAutocomplete = function () {
			if (_company_name_filter) {
				$(_company_name_filter)
					.on("search", function () {
						console.log("Provider.company_name_filter:search()")
						// ----
						
					})
					.on("click", function () {
						console.log("Provider.company_name_filter:click()")
						// ----
						if ($(this).attr("readonly") === "readonly") {
							e.preventDefault()
						} else {
							$(this).select()
						}
					})
					.on("keyup", function () {
						globalSelectedCompany = false
					})
					.on("change", function () {
						console.log("Provider.company_name_filter:change()")
						// ----
						
						let companyName = this.value
						
						setTimeout(function () {
							if (globalSelectedCompany === false && companyName !== "") {
								companyExists(companyName)
							}
						}, 200)
						
					})
					.autocomplete({
						serviceUrl: "/api/v1.0/autocomplete/companies",
						minChars: 2,
						cache: false,
						dataType: "json",
						triggerSelectOnValidInput: false,
						paramName: "st",
						onSelect: function (suggestion) {
							if (!suggestion.data) {
								return
							}
							let company = suggestion.data
							globalSelectedCompany = true
							console.log("|__ company", company)
						},
					})
			}
		}
		
