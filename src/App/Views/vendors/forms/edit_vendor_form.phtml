<!-- Edit Vendor Form -->
<form id="form_edit_vendor" name="form_edit_vendor" class="" novalidate>
    <div class="card mb-2">

        <!-- Card Body -->
        <div class="card-body">

            <h5 class="card-title">
                <span>Vendor Details</span>

            </h5>
            <h6 class="card-subtitle mb-2 text-muted">Panel subtitle</h6>
            <hr class="ml-3 mr-3 mt-1 mb-3 color-dark">

            <div class="row">

                <div class="col-12 col-md-3">
                    <div class="form-element">
                        <label for="vendor_id">Vendor Id:</label>
                        <input type="text" id="vendor_id" name="vendor_id" class="form-control" aria-label="Vendor Id" placeholder="" autocomplete="off" readonly/>
                        <div id="vendor_id-error" class="error w-100 text-center">&nbsp;</div>
                    </div>
                </div>

                <div class="col-12 col-md-3">
                    <div class="form-element">
                        <label for="vendor_company_id">Vendor Company Id:</label>
                        <input type="text" id="vendor_company_id" name="vendor_company_id" class="form-control" aria-label="Provider Id" placeholder="" autocomplete="off" readonly/>
                        <div id="vendor_company_id-error" class="error w-100 text-center">&nbsp;</div>
                    </div>
                </div>

                <div class="col-12 col-md-3">
                    <div class="form-element">
                        <label for="vendor_name">Vendor Name:</label>
                        <input type="text" id="vendor_name" name="vendor_name" class="form-control" aria-label="Vendor Id" placeholder="" autocomplete="off"/>
                        <div id="vendor_name-error" class="error w-100 text-center">&nbsp;</div>
                    </div>
                </div>

                <div class="col-12 col-md-3">
                    <div class="custom-control custom-switch">
                        <input type="checkbox" value="1" class="custom-control-input" id="vendor_enabled">
                        <label class="custom-control-label" for="vendor_enabled">Enabled</label>
                    </div>
                </div>
                '
            </div>

            <div class="row">
                <div class="col-12 col-md-8  col-xl-7">
                    <div class="form-element">
                        <label for="vendor_sku">Vendor SKU:</label>
                        <input type="text" id="vendor_sku" name="vendor_sku" class="form-control" aria-label="Vendor SKU" placeholder="" autocomplete="off"/>
                        <div id="vendor_sku-error" class="error w-100 text-center">&nbsp;</div>
                    </div>

                    <!-- Is Provider -->
                    <div class="custom-control custom-checkbox custom-control-inline">
                        <input type="checkbox" class="custom-control-input" id="vendor_is_provider">
                        <label class="custom-control-label" for="vendor_is_provider">Is Provider</label>
                    </div>
                    <!-- /Is Provider -->

                    <!-- Show Online -->
                    <div class="custom-control custom-checkbox custom-control-inline">
                        <input type="checkbox" class="custom-control-input" id="vendor_show_online">
                        <label class="custom-control-label" for="vendor_show_online">Show Online</label>
                    </div>
                    <!-- /Show Online -->

                    <!-- Show Ops -->
                    <div class="custom-control custom-checkbox custom-control-inline">
                        <input type="checkbox" class="custom-control-input" id="vendor_show_ops">
                        <label class="custom-control-label" for="vendor_show_ops">Show Ops</label>
                    </div>
                    <!-- /Show Ops -->

                    <!-- Show Sales -->
                    <div class="custom-control custom-checkbox custom-control-inline">
                        <input type="checkbox" class="custom-control-input" id="vendor_show_sales">
                        <label class="custom-control-label" for="vendor_show_sales">Show Sales</label>
                    </div>
                    <!-- /Show Sales -->
                </div>
            </div>

            <div class="text-right w-100">
                <a href="javascript:void(0);" id="button_clear_form_edit_vendor" class="btn btn-flat primary-text text-center p-1 mx-0 mb-0 waves-effect waves-light">clear</a>
                <a href="javascript:void(0);" id="button_submit_form_edit_vendor" class="btn btn-primary btn-sm waves-effect waves-light">Save</a>
            </div>

        </div>
        <!-- /Card Body -->

    </div>
</form>
<!-- /Edit Vendor Form -->
